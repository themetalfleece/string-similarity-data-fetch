{
    "name": "string-similarity-data-fetch",
    "version": "0.0.1",
    "description": "An interface which returns data associated with a key based on its similarity with a given string, by querying a MongoDB database",
    "main": "dist/src/index.js",
    "author": "Jason Athanasoglou <jathanasoglou@outlook.com>",
    "license": "MIT",
    "scripts": {
        "dev": "nodemon --watch src/**/*.ts --exec ts-node ./src/app.ts",
        "build": "tsc",
        "start": "node ./dist/app.js",
        "lint": "eslint --fix --quiet src/**/*.ts",
        "format": "prettier --loglevel silent --write src/**/*.ts"
    },
    "dependencies": {
        "@koa/cors": "^3.1.0",
        "@koa/router": "^9.4.0",
        "@types/koa__router": "^8.0.2",
        "dotenv": "^8.2.0",
        "koa": "^2.13.0",
        "koa-bodyparser": "^4.3.0",
        "mongoose": "^5.10.6",
        "string-similarity": "^4.0.2"
    },
    "devDependencies": {
        "@types/dotenv": "^8.2.0",
        "@types/koa": "^2.11.4",
        "@types/koa-bodyparser": "^4.3.0",
        "@types/koa__cors": "^3.0.1",
        "@types/mongoose": "^5.7.36",
        "@types/node": "^14.0.22",
        "@types/string-similarity": "^3.0.0",
        "@typescript-eslint/eslint-plugin": "^3.6.0",
        "@typescript-eslint/parser": "^3.6.0",
        "eslint": "^7.4.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.4",
        "prettier": "^2.0.5",
        "typescript": "^3.9.6",
        "typescript-eslint-parser": "^22.0.0"
    }
}
